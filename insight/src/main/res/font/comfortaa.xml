<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/comfortaa_light"
        android:fontStyle="normal"
        android:fontWeight="300"

        app:font="@font/comfortaa_light"
        app:fontStyle="normal"
        app:fontWeight="300"
        tools:ignore="UnusedAttribute" />
    <font
        android:font="@font/comfortaa_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/comfortaa_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="UnusedAttribute" />
    <font
        android:font="@font/comfortaa_medium"
        android:fontStyle="normal"
        android:fontWeight="500"

        app:font="@font/comfortaa_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute" />
    <font
        android:font="@font/comfortaa_semibold"
        android:fontStyle="normal"
        android:fontWeight="600"

        app:font="@font/comfortaa_semibold"
        app:fontStyle="normal"
        app:fontWeight="600"
        tools:ignore="UnusedAttribute" />
    <font
        android:font="@font/comfortaa_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/comfortaa_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:ignore="UnusedAttribute" />
</font-family>